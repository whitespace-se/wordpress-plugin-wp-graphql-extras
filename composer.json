{
  "name": "whitespace-se/wordpress-plugin-wp-graphql-extras",
  "description": "Wordpress plugin that adds additional types and fields to the GraphQL API",
  "type": "wordpress-plugin",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Whitespace Dev",
      "email": "code@whitespace.se"
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "wp-graphql/wp-graphql": "^1.3",
    "wpackagist-plugin/advanced-custom-fields": "^5.9"
  },
  "extra": {
    "installer-name": "whitespace-wp-graphql-extras"
  }
}
